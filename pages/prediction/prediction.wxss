.header {
    background-color: #fff;
    padding-left: 30rpx;
    padding-bottom: 30rpx;
}

.swiper-tab {
    border-bottom: 1rpx solid #E5E5E5;
    height: 50px;
    text-align: center;
    line-height: 50px;
    color: #999999;
    margin-top: 20rpx;
    background-color: #fff;
}
.swiper-tab-list {
    display: inline-block;
    width: 25%;
    height: 50px;
}

.on {
    color: #353535;
    border-bottom: 6rpx solid #F8CC00;
}

.swiper-box {
    background-color: #fff;
}

.scroll-wrap {
    padding: 0 30rpx;
}
.swiper-item-title {
    font-size: 34rpx;
    color: #333333;
    margin: 40rpx 0 30rpx;
}

.bottom-bar {
    height: 50px;
}